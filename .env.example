# Agent Max Desktop - Environment Configuration
# Copy this file to .env and configure for your deployment

# ===========================================
# CORE APPLICATION SETTINGS
# ===========================================

# API URL
# Development: http://localhost:8000
# Production Railway: https://agentmax-production.up.railway.app
# Custom Domain: https://agentmax.app (after DNS propagates)
VITE_API_URL=https://agentmax-production.up.railway.app

# API Key - Required for authenticated endpoints
# Get this from Railway environment variables (API_KEY)
VITE_API_KEY=your_api_key_here

# Environment
# Options: development, production, beta
VITE_ENVIRONMENT=production

# ===========================================
# PAYMENT SYSTEM (Stripe)
# ===========================================
# Required for onboarding payment step and billing portal
# Get keys from: https://dashboard.stripe.com/apikeys
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key

# ===========================================
# DATABASE (Supabase)
# ===========================================
# Unified database for cross-user caching, memory sync, and session management
# Get credentials from: https://supabase.com/dashboard/project
VITE_SUPABASE_URL=https://rburoajxsyfousnleydw.supabase.co
VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6InJidXJvYWp4c3lmb3VzbmxleWR3Iiwicm9sZSI6ImFub24iLCJpYXQiOjE3NjA5OTE2MTgsImV4cCI6MjA3NjU2NzYxOH0.sWHCQpHiQvI_whjLKF8ybR3mr9BNtPF68MgKT1LLuSc

# ===========================================
# GOOGLE INTEGRATION (Optional)
# ===========================================
# For Google Calendar, Drive, and other integrations
# Get credentials from: https://console.cloud.google.com/
VITE_GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
VITE_GOOGLE_API_KEY=your_google_api_key

# ===========================================
# AMBIGUITY RESOLUTION (Optional)
# ===========================================
# For AI-powered ambiguity resolution system
# Get API key from your ambiguity resolution service
VITE_AMBIGUITY_API_KEY=your_ambiguity_api_key

# ===========================================
# ERROR REPORTING (Optional)
# ===========================================
# Sentry DSN for error tracking and crash reporting
# Get DSN from: https://sentry.io/settings/projects/
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# ===========================================
# FEATURE FLAGS
# ===========================================
# Enable/disable features for different deployment types
VITE_ENABLE_PAYMENTS=true
VITE_ENABLE_GOOGLE_INTEGRATION=true
VITE_ENABLE_AMBIGUITY_RESOLUTION=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_ANALYTICS=false

# ===========================================
# BETA TESTING SETTINGS
# ===========================================
# For beta testers - enable debug features
VITE_DEBUG_MODE=false
VITE_BETA_MODE=true
VITE_UPDATE_CHANNEL=beta

# ===========================================
# PERFORMANCE SETTINGS
# ===========================================
# Hybrid search and embedding settings
VITE_ENABLE_HYBRID_SEARCH=true
VITE_EMBEDDING_CACHE_SIZE=100
VITE_SEARCH_TIMEOUT=5000

# ===========================================
# SECURITY SETTINGS
# ===========================================
# Rate limiting and security
VITE_RATE_LIMIT_REQUESTS=100
VITE_RATE_LIMIT_WINDOW=60000
VITE_ENABLE_CORS=true
