{
  "name": "agent-max-desktop",
  "version": "1.3.3",
  "description": "Desktop application for Agent Max Memory System V2",
  "main": "electron/main/main.cjs",
  "author": "Colin O'Brien",
  "homepage": "https://agentmax.app",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && NODE_ENV=development electron .\"",
    "electron:dev:prodlike": "npm run build && NODE_ENV=production electron .",
    "electron:watch:prodlike": "concurrently \"npm run build:watch\" \"NODE_ENV=production electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:build:mac": "npm run build && electron-builder --mac",
    "electron:build:win": "npm run build && electron-builder --win",
    "electron:build:linux": "npm run build && electron-builder --linux",
    "generate:icons": "chmod +x generate-icons.sh && ./generate-icons.sh",
    "postinstall": "electron-builder install-app-deps",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:jest:coverage": "jest --coverage",
    "test:unit": "vitest run tests/unit",
    "test:e2e": "playwright test",
    "test:e2e:json": "playwright test --reporter=list",
    "test:e2e:triage": "npm run test:e2e:json && node scripts/e2e_triage.mjs",
    "test:api": "playwright test -c playwright.api.config.js",
    "test:api:triage": "npm run test:api -s && node scripts/e2e_triage.mjs",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "node scripts/run-all-tests.js",
    "lint": "eslint . --ext .js,.jsx,.cjs",
    "lint:fix": "eslint . --ext .js,.jsx,.cjs --fix",
    "stylelint": "stylelint \"src/**/*.css\"",
    "stylelint:fix": "stylelint \"src/**/*.css\" --fix",
    "test:a11y": "node scripts/test-a11y.js",
    "test:vr": "node scripts/test-visual-regression.js",
    "test:perf": "node scripts/test-performance.js",
    "test:glass-fallbacks": "node scripts/test-glass-fallbacks.js",
    "test:screenshot": "node scripts/test-screenshot.js",
    "ui:preview": "open test-glass-ui.html",
    "format": "prettier --write \"**/*.{js,jsx,cjs,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,cjs,json,css,md}\"",
    "contracts:sync": "bash scripts/sync_contracts.sh"
  },
  "build": {
    "appId": "com.agentmax.desktop",
    "productName": "Agent Max",
    "copyright": "Copyright Â© 2025 Colin O'Brien",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "build/**/*",
      "package.json"
    ],
    "asarUnpack": [],
    "mac": {
      "icon": "build/icon.icns",
      "category": "public.app-category.productivity",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "resources/entitlements.mac.plist",
      "entitlementsInherit": "resources/entitlements.mac.plist",
      "notarize": {
        "teamId": "Q3Q2BF22GL"
      }
    },
    "win": {
      "icon": "build/icon.ico",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "icon": "build/icon.png",
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ],
      "category": "Utility"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "publish": {
      "provider": "github",
      "owner": "ColinKOB",
      "repo": "agent-max-desktop",
      "private": false,
      "releaseType": "release"
    }
  },
  "dependencies": {
    "@fortune-sheet/formula-parser": "^0.2.13",
    "@fortune-sheet/react": "^1.0.4",
    "@stripe/react-stripe-js": "^5.2.0",
    "@stripe/stripe-js": "^8.1.0",
    "@supabase/supabase-js": "^2.76.0",
    "@tanstack/react-query": "^5.13.4",
    "@xenova/transformers": "^2.17.1",
    "axios": "^1.12.2",
    "axios-retry": "^4.5.0",
    "better-sqlite3": "^9.2.2",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.3",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "eventsource": "^2.0.2",
    "framer-motion": "^12.23.24",
    "keytar": "^7.9.0",
    "lucide-react": "^0.294.0",
    "node-machine-id": "^1.1.12",
    "posthog-js": "^1.306.2",
    "posthog-node": "^5.17.2",
    "puppeteer": "^21.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "react-hot-toast": "^2.4.1",
    "react-hotkeys-hook": "^5.2.1",
    "react-markdown": "^9.1.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.3.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^2.1.0",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.37.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "canvas": "^3.2.0",
    "chalk": "^5.6.2",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.0",
    "lighthouse": "^12.8.2",
    "playwright": "^1.56.1",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "sharp": "^0.34.5",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.4.0",
    "terser": "^5.44.0",
    "to-ico": "^1.1.5",
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "wait-on": "^7.2.0"
  }
}
